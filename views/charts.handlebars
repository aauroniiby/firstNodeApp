    <script type="text/javascript">
      
      
        
    $.get( "https://jsonplaceholder.typicode.com/users", function( respdata ) {
        google.charts.load('current', {'packages':['corechart']});
        google.charts.setOnLoadCallback(drawChart);

        $( "#data" ).append( JSON.stringify(data) ) // John
        function drawChart() {

        var tableData = [['Emri', 'Sasia']];
        for (var item in respdata){
            tableData.push([respdata[item]['name'], respdata[item]['id']])
        }
        console.log(tableData)
        var data = google.visualization.arrayToDataTable(tableData);

        var options = {
          title: 'Chart 1'
        };

        var chart = new google.visualization.PieChart(document.getElementById('piechart'));

        chart.draw(data, options);
      }
